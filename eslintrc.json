{
    "env": { 
        "browser": true,
        "es2021": true 
    },   
    "extends": "airbnb-base/legacy",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11, 
        "sourceType": "module" 
    },
    "plugins": [
        "react", 
        "prettier"
    ],

    "rules": {
        "prettier/prettier": "error", 
        "react/prop-types": 0 
    }
}